{
  "name": "hello-world",
  "version": "2.0.1",
  "main": "dist-electron/main/index.js",
  "description": "Electron Vite React boilerplate with auto update",
  "license": "MIT",
  "private": false,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/",
      "GGS": "fsdsd"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "publish": "vite build && electron-builder -p always",
    "build": "vite build && electron-builder",
    "build:win": "vite build && electron-builder --win --config",
    "build:linux": "vite build && electron-builder --linux --config",
    "preview": "vite preview",
    "pree2e": "vite build --mode=test",
    "e2e": "playwright test"
  },
  "dependencies": {
    "electron-updater": "^6.1.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.37.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.4.5",
    "electron": "^28.1.0",
    "electron-builder": "^24.6.3",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/karthik-raja-i2i/electron-autoupdate-test.git"
  },
  "author": {
    "name": "Karthik",
    "email": "karthickraja@ideas2it.com"
  }
}
